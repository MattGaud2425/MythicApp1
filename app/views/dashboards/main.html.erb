<div class="dashboard-container">
  <div class="row">

    <div class="col-12 dash-top-row">

    </div>
  </div>

  <div class="row">
    <div class="main-dash">
      <div class="col-3 dash-side-nav">
        <%= render "dashboards/side_nav" %>
      </div>
      <div class="col-9 dash-main-body">
        <div class="field">
          <div class="dash-label">Email: </div>
          <div class="label-response"><%= current_user.email %></div>
        </div>
        <div class="field">
          <div class="dash-label">Nickname: </div>
          <div class="label-response"><%= current_user.nickname %></div>
        </div>
      </div>
    </div>
  </div>

</div>
