
<% current_user.player.bracket_pools.all.each do |x| %>
  <% if x.active %>
    <% x.teams.each do |y| %>
      <%= y.team_name %>
    <% end %>
  <% end %>
<% end %>

<% current_user.player.bracket_pool_players.each do |x| %>
  <% x.team.players.each do |y| %>
    <%= y.id %>
  <% end %>
<% end %>

<% current_user.player.bracket_pool_players.each do |x| %>
  <%= x.team.team_name %>
  <% x.team.players.each do |y| %>
    <%= y.nickname %>
  <% end %>
<% end %>
