<%= button_to "Create Bracket Pools", action: :create %>
<%= button_to "Bulk Delete Bracket Pools", action: :destroy %>


<% Bracket.preload(:game).all.each do |x| %>
  <%= x.title %>


  <% x.bracket_pools.where(title: "nightly_free").each do |y| %>
    <%= y.title %> <br>
    <%= button_to "Join Bracket", join_bracket_path(bracket_pool_id: y.id) %>
  <% end %>
  <br>
<% end %>
