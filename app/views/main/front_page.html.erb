<div class="hero-section">
  <div class="hero-box">

    <div class="carousel Wallop">
      <button class="Wallop-buttonNext"><i class="fa fa-cubes"></i></button>
      <button class="Wallop-buttonPrevious"><i class="fa fa-cubes"></i></button>
      <div class="Wallop-list">
        <div class="Wallop-item--current">
        </div>
        <div class="Wallop-item">
          Hello
        </div>
        <div class="Wallop-item">
          Hello
        </div>
        <div class="Wallop-item">
          Hello
        </div>
        
      </div> 
    </div>

    <script>
      // New instance of Wallop
      var slider = document.querySelector('.Wallop');
      var Wallop = new Wallop(slider);
    </script>
    
  </div>
</div>

